<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

  <!-- Drop Shadow Stack -->
  <item>
    <shape>
      <padding android:top="1dp" android:right="1dp" android:bottom="1dp" android:left="1dp" />
      <solid android:color="#05CCCCCC" />
      <corners android:radius="15dp" />
    </shape>
  </item>

  <item>
    <shape>
      <padding android:top="1dp" android:right="1dp" android:bottom="1dp" android:left="1dp" />
      <solid android:color="#0ACCCCCC" />
      <corners android:radius="14dp" />
    </shape>
  </item>

  <item>
    <shape>
      <padding android:top="1dp" android:right="1dp" android:bottom="1dp" android:left="1dp" />
      <solid android:color="#0FCCCCCC" />
      <corners android:radius="13dp" />
    </shape>
  </item>

  <item>
    <shape>
      <padding android:top="1dp" android:right="1dp" android:bottom="1dp" android:left="1dp" />
      <solid android:color="#14CCCCCC" />
      <corners android:radius="12dp" />
    </shape>
  </item>

  <item>
    <shape>
      <padding android:top="0.5dp" android:right="0.5dp" android:bottom="1dp" android:left="0.5dp" />
      <solid android:color="#1ACCCCCC" />
      <corners android:radius="12dp" />
    </shape>
  </item>

  <!-- Background -->
  <item>
    <shape>
      <solid android:color="@android:color/white" />
      <corners android:radius="11dp" />
    </shape>
  </item>
</layer-list>